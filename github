#CREATE
echo "# [NAME]" >> README.md
git init
#COMMIT
*git add .
*git commit -m "info"
git branch -M main
#LINK
git remote add origin https://github.com/MatthiasBenaets/[name].git
#PUSH
*git push -u origin main

* = new commits

##CLONE
git clone https://github.com/MatthiasBenaets/[name].git
cd [name]
git config --global user.email "matthias.benaets@gmail.com"
git config --global user.name "MatthiasBenaets"

#TEMPLATE .git/config
[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
[remote "origin"]
	url = https://www.github.com/MatthiasBenaets/[NAME]
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "main"]
	remote = origin
	merge = refs/heads/main

